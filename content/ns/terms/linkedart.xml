        <rdfs:domain rdf:resource="https://linked.art/ns/terms/PropertyInterest"/>
        <rdfs:range rdf:resource="E39_Actor"/>
</rdf:Property>

<rdf:Property rdf:about="https://linked.art/ns/terms/jurisdiction">
